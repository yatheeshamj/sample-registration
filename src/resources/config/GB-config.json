                "display": true,
                "disabled": false
            },
            "searchByServicePartnerIdButton": {
                "required": false,
                "display": true,
                "inputType": "button",
                "disabled": false
            },
            "viewListButton": {
                "required": false,
                "inputType": "button",
                "disabled": false,
                "display": true
            },
            "display": true,
            "admissionStepId": 7
        },
        "display": true
    },
    "footer": {
