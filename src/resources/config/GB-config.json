{
    "phonenumberValidationScreen": {
        "phoneNumberFormat": {
            "validation": {
                "pattern": "^([0-9]{3})([0-9]{4})([0-9]{4})$",
                "format": "$1-$2 $3",
                "editNumberFormat": "###-#### ####",
                "length": 11,
                "required": true,
                "display": true
            }
        }
    },
